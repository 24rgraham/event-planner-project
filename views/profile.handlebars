<h3 class="text-center">My Events</h3>
<div class="container">
    <div class="row justify-content-center row-cols-1 row-cols-md-3 row-cols-lg-4">
        {{#each events}}
        <div class="card p-2 mb-5" id="event-details" >
            {{#if event_photo}}
            <img src="https://res.cloudinary.com/dqv6cj4bc/image/upload/w_300,h_200,c_fill,q_100/{{event_photo}}" class="card-img-top" alt="...">
            {{else}}
            <img src="https://placebear.com/300/200" class="card-img-top" alt="...">
            {{/if}}
            <div class="card-body">
                <a class="card-text text-capitalize" href="/event/{{id}}">{{name}}</a>
                <p class="card-text"> {{moment eventDate format="dddd, MMMM Do YYYY"}} <br> {{time}} </p>
                <button class="editEvent btn mt-2 border-secondary button text-secondary" data-eventId="{{id}}">Edit Event</button>
                <button class="deleteEvent btn mt-2 border-danger button text-danger" data-eventId="{{id}}">Delete Event</button>
            </div>
        </div>
        {{/each}}


        <script src="/js/profile.js"></script>